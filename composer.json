{
    "name": "silverstripe-terraformers/silverstripe-twig",
    "description": "Allows use of twig in SilverStripe 3.1",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe", "twig", "templating"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "SilverStripe Ltd"
        },
        {
            "name": "Silverstripe Terraformers",
            "email": "terraformers@silverstripe.com"
        }
    ],
    "support": {
        "issues": "https://github.com/silverstripe-terraformers/silverstripe-twig/issues"
    },
    "require": {
        "php": ">=7.3",
        "silverstripe/vendor-plugin": "^1.0",
        "silverstripe/framework": "^4@dev",
        "silverstripe/cms": "^4@dev",
        "twig/twig": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Terraformers\\Twig\\": "src/",
            "Terraformers\\Twig\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
